{{#section 'jquery'}}
<script>
    menu.setActive(2);
    var list= document.getElementsByClassName("circle_div")
    for (var i = 0; i < list.length; i++) {
        var ele = list[i];
        var currentTop = ele.style.top;
        currentTop = parseInt(currentTop.replace("px", ""));
        ele.style.top = (currentTop - 42) + "px";
    }
    var timerID_update, timerID_reload;

    $(document).ready(function () {
        $("a#menu8").addClass("current");
        header.setTitle('CNC Robot Line Status');
        footer.setTitle();
        //$('#title').text('CNC Robot Line Status');
        $("div.line_button_div").remove();

        timerID_reload = setTimeout(reload_view, 1 * 60 * 60 * 1000); // 1시간에 한번씩 화면 Reload

        $(document).on('click', 'input.button', function () {
            $.ajax({
                url: '/ajax/select-line/' + $(this).attr('id'),
                type: 'GET',
                timeout: 3000,
                success: function (data) {
                    let url = '/line-status/' + data.groupId + '/' + data.line_code;
                    window.location.href = url;
                },
                error: function (err) {
                    console.log(err);
                }
            });
            //window.location.href = '/line-status/' + $(this).attr('id');
        });

        btn_enable();
        updateView();

        function btn_enable() {
            $.ajax({
                url: '/ajax/layout-get',
                type: 'GET',
                timeout: 60000,
                success: function (data) {
                    var btnID;
                    for (var i = 0; i < data[1].length; i++) {
                        btnID = '#' + data[1][i].RobotLineCode;
                        $(btnID).prop('disabled', false);
                    }
                },
                error: function (err) {
                    console.log(err);
                }
            });
        }

        function updateView() {
            clearTimeout(timerID_update);

            function lightDisplay(data) {
                var main_title = data.title;
                var prod_data = data.summary;
                var robot_util_data = data.robotRunRate;
                var cnc_util_data = data.cncRunRate;

                //$('#cur_datetime').text(main_title.cur_datetime + ' [T : 10sec]');
                footer.setTitle(main_title.cur_datetime + ' [T : 10sec]');

                var status = "";
                for (var i = 0; i < prod_data.length; i++) {
                    $('#box' + prod_data[i].RobotLineCode).css('border-color', 'darkgray');
                    $('#g' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    $('#y' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    $('#r' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');

                    // Border 부분
                    //달성률이 95% 미만이면 빨간불
                    if (prod_data[i].prod_cnt / prod_data[i].target_cnt * 100 < 95) {
                        $('#box' + prod_data[i].RobotLineCode).css('border-color', 'red');
                        //$('#g'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        //$('#y'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        //$('#r'+prod_data[i].RobotLineCode).css('background-color', 'red');
                    }
                    //로봇 가동률이나 CNC 가동률이 95% 미만이면 노란불
                    else if (cnc_util_data[i].cnc_run_rate < 95 || robot_util_data[i].robot_run_rate < 95) {
                        // console.log(prod_data[i].RobotLineCode + ' / ' + cnc_util_data[i].cnc_run_rate+ ' / '+robot_util_data[i].robot_run_rate);
                        $('#box' + prod_data[i].RobotLineCode).css('border-color', 'yellow');
                        //$('#g'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        //$('#y'+prod_data[i].RobotLineCode).css('background-color', 'yellow');
                        //$('#r'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }
                    //그외 녹색불
                    else {
                        $('#box' + prod_data[i].RobotLineCode).css('border-color', 'green');
                        //$('#g'+prod_data[i].RobotLineCode).css('background-color', 'green');
                        //$('#y'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        //$('#r'+prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }

                    //RSS 상태 부분
                    // 0: RUN
                    status = status + "_" + robot_util_data[i].robot_status;
                    if (robot_util_data[i].robot_status == 0) {
                        $('#g' + prod_data[i].RobotLineCode).css('background-color', 'green');
                        $('#y' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        $('#r' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }
                    // 1: READY
                    else if (robot_util_data[i].robot_status == 1) {
                        $('#g' + prod_data[i].RobotLineCode).css('background-color', 'green');
                        $('#y' + prod_data[i].RobotLineCode).css('background-color', 'orange');
                        $('#r' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }
                    // 2: STOP
                    else if (robot_util_data[i].robot_status == 2) {
                        $('#g' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        $('#y' + prod_data[i].RobotLineCode).css('background-color', 'orange');
                        $('#r' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }
                    // 3: ALARM
                    else if (robot_util_data[i].robot_status == 3) {
                        $('#g' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        $('#y' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        $('#r' + prod_data[i].RobotLineCode).css('background-color', 'red');
                    }
                    // 4: MANUAL
                    else if (robot_util_data[i].robot_status == 4) {
                        $('#g' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                        $('#y' + prod_data[i].RobotLineCode).css('background-color', 'yellow');
                        $('#r' + prod_data[i].RobotLineCode).css('background-color', 'darkgray');
                    }
                }

                //console.log(status + "_" + prod_data.length);

            }

            $.ajax({
                url: '/ajax/layout-get',
                type: 'GET',
                timeout: 60000,
                success: function (data) {
                    lightDisplay(data);
                    timerID_update = setTimeout(updateView, 300);
                },
                error: function (err) {
                    console.log(err);
                    timerID_update = setTimeout(updateView, 10000);
                }
            });
        };

        function reload_view() {
            clearTimeout(timerID_reload);
            var url = "/layout";
            $(location).attr('href', url);
        }
    });

</script>

