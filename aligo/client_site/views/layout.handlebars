<style>
    .t_summary {
        /*border: 2px solid rgb(100, 100, 100);*/
        border: 2px solid #0088dd;
        width: 99.3%;
        height: 90px;
        margin-top: 20px;
        margin-bottom: 10px;

        margin-left: 0.3%;
        margin-right: auto;
        empty-cells: show;
        font-size: 40px;
    }

    .t_header {
        /*background-color: rgb(100, 100, 100);*/
        background-color: steelblue;
        width: 200px;
        text-align: center;
        font-weight: bold;
        color: white;
    }

    .t_data {
        /*background-color: rgba(204, 236, 255, 0.4);*/
        text-align: right;
        font-size: 40px;
        font-weight: bold;
        width: 190px;
        padding-right: 8px;
    }
</style>


<style>
    .d3_div {
        border: 2px solid #0088dd;
        width: 1895px;
        height: 890px;
        margin-left: 10px;
        margin-top: 10px;
        margin-bottom: 10px;
        background-image: url("../img/layout_all.png");
    }
</style>



<style>
    .circle_div {
        position: absolute;
        border: 3px solid gray;
        background-color: black;
        padding: 2px;
    }

    .circle {
        float: left;
        width: 16px;
        height: 16px;
        background: darkgray;
        border: 2px solid black;
        -moz-border-radius: 8px;
        -webkit-border-radius: 8px;
        border-radius: 8px;
    }

    .button_div {
        clear: left;
    }

    .button {
        width: 60px;
        font-weight: bold;
    }
</style>

